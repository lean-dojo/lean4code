{
  "name": "lean4dojo-panel",
  "displayName": "LeanDojo Panel",
  "description": "A custom panel for LeanDojo",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:leanDojoView"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "leanDojoViewContainer",
          "title": "LeanDojo",
          "icon": "resources/icon.svg"
        }
      ]
    },
    "views": {
      "leanDojoViewContainer": [
        {
          "type": "webview",
          "id": "leanDojoView",
          "name": "LeanDojo View"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.60.0",
    "@types/node": "^16.11.7",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "eslint": "^8.13.0",
    "typescript": "^4.7.2"
  }
} 